{
  "name": "freelance_app",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api/*": {
      "target": "https://app-freelance.herokuapp.com"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "nodemon": "^1.15.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0",
    "url-loader": "^1.0.1"
  },
  "scripts": {
    "start": "node index.js",
    "client": "npm run start ",
    "server": "node server.js --prefix api-mongo-server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix api-mongo-server && npm run build"
  }
}
